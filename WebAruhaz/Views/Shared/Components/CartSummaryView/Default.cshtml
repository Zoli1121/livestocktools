@using WebAruhaz.Models.ViewModels.WebShopViewModels

@model CartViewModel

@if (Model.Cart.ShoppingLists.Count > 0 )
{
    <li>
        <a asp-controller="Cart">
            <span class="glyphicon glyphicon-cart"></span>
            <span id="cart-status">
                @Model.Cart.ShoppingLists.Count
            </span>
        </a>
    </li>
}